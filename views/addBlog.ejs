<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>AddNewBlogPage</title>
</head>
<body>
    <%- include('./partials/navbar') %> 
    <form method="POST" action="/blog/add-Blog" enctype="multipart/form-data">


        <div class="mb-3 mt-4">
            <label for="coverImage" >coverImage</label>
            <input type="file" class="form-control" id="coverImage" name="coverImage" aria-describedby="coverImage">
        </div>

        <div class="mb-3 ">
            <label for="title" >Title</label>
            <input type="title" class="form-control" id="title" name="title" aria-describedby="title">
        </div>

        <div class="mb-3">
            <label for="content" >Content</label>
            <textarea class="form-control"  id="content" name = "content"></textarea>
        </div>

        <div class="mb-3">
            <button class="btn btn-primary" >Submit</button>
        </div>

    </form>
    


    <%- include('./partials/scripts') %> 
</body>
</html>